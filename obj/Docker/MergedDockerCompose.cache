networks:
  default:
    external:
      name: nat
services:
  webapitest:
    build:
      context: D:\Projects\DemoApplication
      dockerfile: WebAPITest/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: webapitest
    ports:
    - published: 44307
      target: 80
    - published: 44307
      target: 443
    volumes:
    - C:\Users\vinhton\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\vinhton\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
version: '3.4'